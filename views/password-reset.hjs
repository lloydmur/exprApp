    <title>Welcome to this WebApp</title>
  </head>
  <body>
      <div class="container">
        <h1>Reset Pasword</h1>
        <form class="" action="/account/newpassword" method="post">
          <input type="password" name="password1" placeholder='Enter New Password' value=""><br>
          <input type="password" name="password2" placeholder='Re-Enter Password' value=""> <br>
          <input type="hidden" name="nonce" value={{nonce}}>
          <input type="hidden" name="id" value={{id}}>
          <input type="submit" value="Reset Password">
        </form>

  </body>
</html>
